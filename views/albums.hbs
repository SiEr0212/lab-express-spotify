<h3>Albums</h3>

<div class="generalStyle">
    <div class="albumDisplay">
        {{#each albums}}
            <img src="{{this.images.0.url}}" alt="image">
            {{this.name}}
            <form action="/artist-search" method="GET">
            <a href="/tracks/{{this.id}}">View Tracks</a>
        {{/each}}
    </div>
</div> 