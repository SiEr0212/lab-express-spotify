<h3>Tracks</h3>

<div class="generalStyle">
    <div class="tracksDisplay">
        {{#each tracks}}
            {{this.name}}
            <figure>

            <audio controls src="{{this.preview_url}}">
                Your browser does not support the 
                <code>audio</code> element.
            </audio>
        </figure>
        {{/each}}
    </div>
</div> 